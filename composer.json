{
    "name": "braseidon/steam-inventory-laravel",
    "description": "SteamInventory is a wrapper around the JSON Steam API that grabs a user's items - all packaged up for Laravel.",
    "type": "library",
    "keywords": ["steam", "api", "inventory", "wrapper", "json", "steamid", "php", "laravel"],
    "homepage": "https://github.com/braseidon/steam-inventory-laravel",
    "authors": [
        {
            "name": "Brandon Johnson",
            "email": "bluejd910@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "5.0 - 5.2"
    },
    "autoload": {
        "psr-4": {
            "Braseidon\\SteamInventory\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
